article .article_header{height:23px;border-bottom:1px solid #d7d7d7;font-size:11px;color:#8d8d8d;}
article .article_header time{float:left;padding:0 9px 0 0;line-height:14px;border-right:1px solid #e0e0e0;}
article .article_header a{line-height:14px;text-decoration:none;}
article .article_header a:hover{text-decoration:underline;}
article .article_header a.article_author{float:left;color:#8f8f8f;padding:0 0 0 5px;font-weight:600;}
article .article_header a.article_comments{float:right;color:#8f8f8f;}
article .article_body{}
/*article .article_body a{text-decoration:none;color:#555;}
article .article_body a:hover{text-decoration:underline;}*/
article .article_body h3{margin:0;padding:8px 0 15px;color:#1c9dd6;font-weight:400;font-size:24px;}
article .article_body h3.compressed{padding:8px 0 0px;}
article .article_body h4{margin:0;padding-bottom:15px;font-size:18px;}
article .article_body h3 a{color:#1c9dd6;text-decoration:none;}
article .article_body .article_content{line-height:160%;}
article .article_body .article_content ul{line-height:100%;list-style:square;}
article .article_body .image{border:1px solid #dfdfdf;background:#f8f8f8;padding:6px;}
article .article_body .left{float:left;margin:0 15px 10px 0;}
article .article_body .right{float:right;margin:0 0 10px 15px;}
article .article_body .image img{display:block;}
article .article_body .article_content p{padding-bottom:14px;}
article .article_body .article_content a.read_more{display:inline-block;padding:2px 10px 3px;background:#1c93bb;border:1px solid #1387ac;border-radius:2px;font-size:12px;color:#fff;text-decoration:none;font-weight:600;}
article .article_body .article_content a.read_more{
  gradient-radial: #20a7d6, #1691ba
}
// article .article_body .article_content a.read_more:hover{}
article .article_body hr{width:90%;border-top:1px solid #EEE;border-bottom:1px solid white;border-right:1px solid white;border-left:1px solid white;}
article .article_body dl{margin:0;padding:0;}
article .article_body dt{font-size:15px;}
article .article_body dd{color:#555;}
article .article_body dd ul{margin-top:3px;padding-left:15px;}

#articles .pagination{margin:20px 0 0;padding:8px 18px;border:1px solid #e1e1e1;background:#fff;}
#articles .pagination nav{float:right;font-size:11px;}
#articles .pagination nav a{text-decoration:none;padding:0 10px;border-right:1px solid #eee;line-height:100%;}
#articles .pagination nav a:hover{text-decoration:underline;}
#articles .pagination nav a.active{font-weight:600;color:#1c9dd6;cursor:default;}
#articles .pagination nav a.last{border-right:0;}
#articles .pagination nav a.active:hover{text-decoration:none;}
#articles .pagination nav a.pagination_start{font-size:11px;}
#articles .pagination nav a.pagination_end{font-size:11px;}
#articles .pagination span.pagination_counter{color:#6e6e6e;}
#articles .pagination span.pagination_counter b{color:#229dc7;font-weight:600;}

#articles.table_block{width:612px;float:right;}
article table{width:100%;border:1px solid #eee;font-size:13px;vertical-align:middle;table-layout:fixed;margin-bottom:20px;}
article table tr{border-bottom:1px solid #eee;}
article table th, td{border-left:1px solid #eee;padding:8px;line-height:1.5em;}
article table th.name, td.name{width:160px;}
article table th.degree, td.degree{width:100px;}
article table thead tr{background:whiteSmoke;}
article table tbody tr.person{cursor:pointer;}
article table tbody tr.person:hover{background:whiteSmoke;}
article table a{text-decoration:none;}
article table a:hover{text-decoration:underline;}
article table tr.id_card{background:url('../images/lightpaperfibers.png') repeat;}
article table tr.id_card div.card{padding:7px;}
article table tr.id_card img{float:left;}
article table tr.id_card div{margin:0px;}
article table tr.id_card div.id_right{float:left;margin-left:50px;}
article table tr.id_card h4{font-size:19px;margin:0 0 5px;}
article table tr.id_card p{line-height:1.2em;}
article table tr.id_card .emphasis{font-size:15px;font-weight:600;}
article table tr.id_card ul{margin-top:0;}
article table tr.id_card li{list-style-type:square;}
a.hide_btn{padding:2px 15px 3px;background:#777;border:1px solid #555;border-radius:2px;font-size:12px;color:#fff;text-decoration:none;font-weight:600;float:right;margin-right:15px;}
a.hide_btn:hover{text-decoration:none;}

.timeline_main{width: 962px;min-height: 100%;overflow: hidden;position: relative;margin: 20px auto;padding: 0;background: none;}
.timeline_main #articles.content{width: 433px;padding: 15px;margin: 15px 0 0px;position: relative;overflow: visible;z-index: 5;}
.timeline_main #articles.content.first, .timeline_main #articles.content.second{margin: 0;}
.timeline_main #articles.content.left {float: left;}
.timeline_main #articles.content.right{float: right;}
.timeline_main #articles.content.long{width: 930px;margin-right:0px;}
.timeline_main #articles.content .arrow{width: 0;height: 0;border: 12px solid rgb(225,225,225);content: " ";overflow: visible;z-index: 3;}
.timeline_main #articles.content.right .arrow, .timeline_main #articles.content.left .arrow{position: absolute;top: 35px;right: -12px;border-top: 12px solid transparent;border-bottom: 12px solid transparent;}
.timeline_main #articles.content.left .arrow{border-right: 0;}
.timeline_main #articles.content .arrow .circle{width: 8px;height: 8px;-webkit-border-radius: 50%;-moz-border-radius: 50%;-o-border-radius: 50%;border-radius: 16px;background: gray;border: 2px solid rgb(238, 238, 238);}
.timeline_main #articles.content .arrow_inside{width: 0;height: 0;z-index: 4;border: 12px solid white;position: absolute;}
.timeline_main #articles.content.left .arrow_inside{position: absolute;top: 35px;right: -11px;border-right: 0;border-bottom: 12px solid transparent;border-top: 12px solid transparent; }
.timeline_main #articles.content.right .arrow, .timeline_main #articles.content.right .arrow_inside{top:60px;left: -11px;border-left: 0;border-bottom: 12px solid transparent;border-top: 12px solid transparent; }
.timeline_main #articles.content.right .arrow{border-right-color: rgb(225,225,225);border-left: 0;left: -12px;}
.timeline_main #articles.content.long.first .arrow, .timeline_main #articles.content.long.first .arrow_inside{border-bottom: 0;border-left: 12px solid transparent;border-right: 12px solid transparent;position: relative;top: 39px;left: 453px;}
.timeline_main #articles.content.long.first .arrow{border-top: 12px solid #e1e1e1;}
.timeline_main #articles.content.long.first .arrow_inside{border-top: 12px solid white;position: relative;top:26px;}
.timeline_main #articles.content.long.first .arrow .circle{position: absolute;top: 5px;left: -6px;}
.timeline_main #articles.content.long .arrow_inside,
.timeline_main #articles.content.long .arrow,
.timeline_main #articles.content.long.right .arrow,
.timeline_main #articles.content.long.left .arrow{border-left: 12px solid transparent;border-right: 12px solid transparent;border-top: 0;position: absolute;top: -13px;left: 468px;}
.timeline_main #articles.content.long .arrow_inside{border-bottom-color: white;top: -12px;}
.timeline_main #articles.content.long .arrow .circle, .timeline_main #articles.content.long.right .arrow .circle, .timeline_main #articles.content.long.left .arrow .circle{position:absolute;top: -15px;right: -6px;}
.timeline_main #articles.content.right .arrow .circle, .timeline_main #articles.content.left .arrow .circle {position: absolute;top: -6px;right: -11px;}
.timeline_main .mid_line{width:6px;background: gray;height: 100%;overflow: hidden;margin-left: 3px;left: 475px;z-index: 2;position: absolute;border-radius: 9px;-webkit-border-radius: 9px;-moz-border-radius: 9px;-o-border-radius: 9px;}

.timeline_main #articles.content.right .arrow .circle{left: -11px;}

.timeline_main .no-content{width:350px;margin:0 auto;font-size:18px;text-align:center;}
